class Solution:
    def trap(self, height: List[int]) -> int:
        a = height[:height.index(max(height))+1]
        b = height[height.index(max(height)):]
        r1max = max(a)
        l1max = max(b)
        water = 0
        for i in range(1, len(a) - 1):
            lmax = max(a[:i])
            twater = min(lmax, r1max) - a[i]
            if twater > 0:
                water += twater
        for i in range(1, len(b) - 1):
            rmax = max(b[i + 1:])
            twater = min(l1max, rmax) - b[i]
            if twater > 0:
                water += twater
        return(water)
